define:*,
{
	draw_line:0.0,0.5,	1.0,0.5;
	draw_line:0.0,1.0,	1.0,0.0;
	draw_line:0.0,0.0,	1.0,1.0;
};

define:-,
{
	draw_line:0.0,0.5,	1.0,0.5;
};

define:_,
{
	draw_line:0.0,0.0,	1.0,0.0;
};

define:+,
{
	draw_line:0.0,0.5,	1.0,0.5;
	draw_line:0.5,1.0,	0.5,0.0;
};

define:/,
{
	draw_line:0.0,0.0,	1.0,1.0;
};